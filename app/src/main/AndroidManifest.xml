<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dit.cs.ljh.moonproject">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="달보기"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Moon_Today" />
        <activity android:name=".Moon_Month" />
        <activity android:name=".Constellation_Now_Season" />
        <activity android:name=".Solar_System" />
        <activity android:name=".Constellation_Detail" />
        <activity android:name=".Planet_detail" />
        <activity android:name=".Constellation_List"></activity>
    </application>

</manifest>